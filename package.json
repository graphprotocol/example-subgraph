{
  "name": "example",
  "version": "0.1.0",
  "repository": "https://github.com/graphprotocol/example-subgraph",
  "license": "MIT",
  "scripts": {
    "build-contract": "solc contracts/Counter.sol --abi -o abis --overwrite && solc contracts/Counter.sol --bin -o bin --overwrite",
    "create": "graph create example --node https://api.staging.thegraph.com/deploy/",
    "create-local": "graph create example --node http://127.0.0.1:8020",
    "codegen": "graph codegen --debug --output-dir src/types/",
    "build": "graph build --debug",
    "deploy": "graph deploy example --debug --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "deploy-local": "graph deploy example --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.7.0",
    "@graphprotocol/graph-ts": "^0.5.0"
  }
}
